{
  "name": "viomedia",
  "private": true,
  "bin": "dist/server/index.js",
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json server/index.ts",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "build": "npm run build:next && npm run build:server",
    "start:prod": "NODE_ENV=production node dist/server/index.js",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/ip": "^1.1.0",
    "@types/node-notifier": "^8.0.2",
    "body-parser": "^1.19.1",
    "buffer": "^6.0.3",
    "child_process": "^1.0.2",
    "express": "^4.17.2",
    "fs": "^0.0.1-security",
    "hidefile": "^3.0.0",
    "ip": "^1.1.5",
    "next": "12.0.10",
    "node-loader": "^2.0.0",
    "node-notifier": "^10.0.1",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "pkg": "^5.5.2",
    "process": "^0.11.10",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/hidefile": "^3.0.0",
    "@types/node": "17.0.16",
    "@types/react": "17.0.39",
    "autoprefixer": "^10.4.2",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.23",
    "typescript": "4.5.5"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": "dist/**/*",
    "targets": [
      "latest-linux-x64",
      "latest-win-x64",
      "latest-macos-x64"
    ],
    "outputPath": "build"
  }
